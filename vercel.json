{
    "version": 2,
    "builds": [
        {"src": "app.py", "use": "@vercel/python"}
    ],
    "routes": [
        {"src": "/(.*)", "dest": "app.py"}
    ],
    "functions": {
        "app/api/**/*": {
            "maxDuration": 5 // All functions can run for a maximum of 5 seconds
        }
    }
}
